{
    "name": "codegreencreative/laravel-aweber",
    "description": "Laravel OAuth Aweber",
    "homepage": "https://codegreencreative.com/",
    "license": "MIT License",
    "require": {
        "php": "^5.3|^5.6|^7.0",
        "ext-json": "*",
        "ext-hash": "*",
        "illuminate/support": "^4.1|^5.0|^6.0|^7.0",
        "illuminate/routing": "^4.1|^5.6|^6.0|^7.0",
        "league/oauth2-client": "~2.4.1"
    },
    "autoload": {
        "psr-4": {
            "CodeGreenCreative\\Aweber\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "CodeGreenCreative\\Aweber\\AweberServiceProvider"
            ],
            "aliases": {
                "Aweber": "CodeGreenCreative\\Aweber\\AweberFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}