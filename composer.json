{
    "name": "codegreencreative/laravel-aweber",
    "description": "Laravel OAuth Aweber",
    "homepage": "https://codegreencreative.com/",
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "ext-hash": "*",
        "laravel/framework": ">=5.7"
    },
    "autoload": {
        "psr-4": {
            "CodeGreenCreative\\Aweber\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CodeGreenCreative\\Aweber\\AweberServiceProvider"
            ],
            "aliases": {
                "Aweber": "CodeGreenCreative\\Aweber\\AweberFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
